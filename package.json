{
  "name": "esgestaosalas",
  "version": "1.0.0",
  "description": "Projeto da cadeira de Engenharia de Software. Iremos implementar uma nova versão de funcionalidade de gestão de salas em Web.",
  "main": "script.js",
  "scripts": {
    "lint": "eslint src/script.js",
    "test": "jest --coverage",
    "doc": "jsdoc -c jsdoc.json",
    "prerun": "npx webpack",
    "run": "npm start"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "d3": "^7.9.0",
    "express": "^4.18.3",
    "papaparse": "^5.4.1",
    "tabulator-tables": "^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-complexity": "^1.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [

      "src/DateConverter.js",
      "src/ClassScheduler.js"
      
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
    
  }
}
